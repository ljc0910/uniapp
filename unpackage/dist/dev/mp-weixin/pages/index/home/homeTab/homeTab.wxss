.home-tab-tpl {
  padding: 0 40rpx 100rpx;
}
.home-tab-tpl .home-nav {
  width: 500rpx;
  padding: 0;
}
.home-tab-tpl .home-tab {
  font-size: 32rpx;
  color: #333333;
  text-align: left;
  padding-left: 0;
  padding-right: 62rpx;
}
.home-tab-tpl .home-tab-active {
  font-size: 36rpx;
  font-weight: bold;
  position: relative;
}
.home-tab-tpl .home-tab-active::after {
  content: "";
  display: block;
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  background: url(data:image/png;base64,);
  background-size: cover;
  right: 50rpx;
  top: 38rpx;
  z-index: 1;
  opacity: 0.7;
}
.home-tab-tpl .tab-filter {
  color: red;
  background: #fff;
  width: 126rpx;
  line-height: 88rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 32rpx;
  color: #333333;
}
.home-tab-tpl .tab-filter ._img {
  width: 28rpx;
  height: 28rpx;
}

